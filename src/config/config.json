{
  "site": {
    "title": "Fundación Rescate de Mascotas Chile",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/logo.png",
    "logo_darkmode": "/images/logo-darkmode.png",
    "logo_width": "250",
    "logo_height": "50",
    "logo_text": "Rescate de Mascotas Chile"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 2,
    "summary_length": 200,
    "blog_folder": "blog"
  },

  "announcement": {
    "enable": true,
    "content": "<span class='text-center block'>♥️ **Aqui pueden ponerse enlaces al proyecto de coronas** <a class='underline' href='https://github.com/zeon-studio/astroplate' target='_blank' rel='noopener'>Please ⭐️ on Github</a></span>",
    "expire_days": 360
  },


  "params": {
    "contact_form_action": "#",
    "copyright": "Implementado por MPZ \uD83E\uDD99"
  },

  "navigation_button": {
    "enable": true,
    "label": "Enlace Proyecto Coronas",
    "link": "https://github.com/zeon-studio/astroplate"
  },

  "google_tag_manager": {
    "enable": false,
    "gtm_id": "GTM-XXXXXX"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "zeon.studio",
    "meta_image": "/images/og-image.png",
    "meta_description": "astro and tailwind boilerplate"
  }
}
